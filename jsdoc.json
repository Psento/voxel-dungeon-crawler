
{
    "source": {
      "include": ["server", "client", "game", "shared"],
      "exclude": ["node_modules"]
    },
    "plugins": ["plugins/markdown"],
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    },
    "opts": {
      "destination": "./docs/",
      "recurse": true,
      "readme": "./README.md"
    }
  }